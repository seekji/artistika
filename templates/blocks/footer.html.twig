<section class="feedback">
    <div class="feedback__container">
        <form action="{{ path('api.subscribe.add') }}" class="feedback__form">
            <div class="feedback__form-field form-field">
                <input type="email" placeholder="Укажите эл. почту" required name="email" class="custom-input">
            </div>

            <div class="feedback__form-field-actions">
                <div class="feedback__form-field-battons">
                    <button class="button button_size_m button_theme_default button_with_icon">
                        <svg class="icon icon-plain" width="32" height="31">
                            <use xlink:href="#icon-plain" />
                        </svg>
                        Отправьте
                    </button>

                    <span class="form-hint">
                        Нажимая эту кнопку вы соглашаетесь с правилами обработки персональных данных
                    </span>
                </div>

                <div class="feedback__welcome">
                    Будьте в курсе самых важных событий Новосибирска
                </div>
            </div>
        </form>

        <div class="feedback__social social">
            {% if getSiteSettingByKey('facebook') is not null %}
                <a href="{{ getSiteSettingByKey('facebook') }}" class="social__link" target="_blank">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 51.85 51.85">
                        <path fill="none" stroke="#fff" stroke-miterlimit="10" stroke-width="3.5" d="M25.92 1.75A24.18 24.18 0 1 0 50.1 25.92 24.17 24.17 0 0 0 25.92 1.75z"/>
                        <path fill="#fff" d="M32.59 16.23h-3.52a1.32 1.32 0 0 0-1.38 1.51V21h4.88L32 26.56h-4.31v15.8h-6.54v-15.8H18V21h3.11v-3.59c0-2.57 1.22-6.6 6.6-6.6h4.84z"/>
                    </svg>
                    <span class="social__link-text">Facebook</span>
                </a>
            {% endif %}
            {% if getSiteSettingByKey('vk') is not null %}
                <a href="{{ getSiteSettingByKey('vk') }}" class="social__link" target="_blank">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 51.55 51.55">
                        <path fill="none" stroke="#fff" stroke-miterlimit="10" stroke-width="3.5" d="M25.78 1.75a24 24 0 1 0 24 24 24 24 0 0 0-24-24z"/>
                        <path fill="#fff" d="M37.79 28.53c1.12 1.1 2.3 2.13 3.31 3.33a10 10 0 0 1 1.18 1.71c.46.89 0 1.86-.75 1.91h-4.92a3.84 3.84 0 0 1-3.14-1.28c-.68-.69-1.31-1.43-2-2.15a4.8 4.8 0 0 0-.89-.79 1 1 0 0 0-1.64.4 5.26 5.26 0 0 0-.52 2.3c-.05 1.16-.4 1.46-1.57 1.52a12.63 12.63 0 0 1-7-1.52 15.55 15.55 0 0 1-4.74-4.43A59.86 59.86 0 0 1 8.87 18.4c-.39-.87-.1-1.34.86-1.36h4.82a1.41 1.41 0 0 1 1.34 1 29.45 29.45 0 0 0 3.27 6.05 4.85 4.85 0 0 0 1.23 1.36.75.75 0 0 0 1.27-.37A4 4 0 0 0 22 23.8a18.84 18.84 0 0 0-.08-4.44 1.85 1.85 0 0 0-1.58-1.7c-.47-.09-.4-.27-.17-.53a1.74 1.74 0 0 1 1.51-.75h5.56c.88.18 1.07.57 1.19 1.45V24c0 .34.17 1.36.78 1.58s.82-.23 1.11-.54a19.41 19.41 0 0 0 3.14-4.82c.37-.76.7-1.55 1-2.34a1.2 1.2 0 0 1 1.23-.88H41a3.87 3.87 0 0 1 .48 0c.9.16 1.15.55.87 1.43a13.6 13.6 0 0 1-2.13 3.69c-.89 1.23-1.85 2.43-2.73 3.67s-.71 1.76.3 2.74z"/>
                    </svg>
                    <span class="social__link-text">Vkontakte</span>
                </a>
            {% endif %}
            {% if getSiteSettingByKey('instagram') is not null %}
                <a href="{{ getSiteSettingByKey('instagram') }}" class="social__link" target="_blank">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48.5 48.48">
                        <path fill="none" stroke="#fff" stroke-miterlimit="10" stroke-width="3.5" d="M44 43.94a7.17 7.17 0 0 0 1.86-4 121.13 121.13 0 0 0 .94-15.67 121.2 121.2 0 0 0-.94-15.66 7.33 7.33 0 0 0-1.86-4 7.37 7.37 0 0 0-4-1.86 123.18 123.18 0 0 0-15.67-.94 122.89 122.89 0 0 0-15.66.94 7.29 7.29 0 0 0-4 1.86 7.3 7.3 0 0 0-1.86 4 121.08 121.08 0 0 0-.94 15.66 121.08 121.08 0 0 0 .82 15.63 7.3 7.3 0 0 0 1.86 4 7.3 7.3 0 0 0 4 1.86 121.25 121.25 0 0 0 15.67.94 121.2 121.2 0 0 0 15.66-.94A7.35 7.35 0 0 0 44 43.94z" class="cls-1"/>
                        <path fill="none" stroke="#fff" stroke-miterlimit="10" stroke-width="3.5" d="M35.43 24.24a11.18 11.18 0 1 1-11.18-11.18 11.18 11.18 0 0 1 11.18 11.18z"/>
                        <path fill="#fff" d="M40.44 10.74a2.69 2.69 0 1 1-2.69-2.69 2.7 2.7 0 0 1 2.69 2.69z"/>
                    </svg>
                    <span class="social__link-text">Instagram</span>
                </a>
            {% endif %}
            <span class="social__title"> &mdash; Давайте дружить</span>
        </div>
    </div>
    <div class="notification notification_error">
        <div class="notification__close js-close-notification">
            <svg class="icon icon-close" width="28" height="28">
                <use xlink:href="#icon-close" />
            </svg>
        </div>

        <div class="notification__content">
            <div class="notification__information">
                <div class="notification__heading">
                    ОЙ!
                </div>

                <div class="notification__description">
                    <div class="notification__title">
                        Кажется вы неправильно указали
                        адрес электронной почты
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="notification notification_success">
        <div class="notification__close">
            <svg class="icon icon-close" width="28" height="28">
                <use xlink:href="#icon-close" />
            </svg>
        </div>

        <div class="notification__content">
            <div class="notification__information">
                <div class="notification__heading">
                    СПАСИБО!
                </div>

                <div class="notification__description">
                    <div class="notification__title">
                        Мы будем присылать вам
                        только самые важные
                        и интересные новости
                    </div>

                    <div class="notification__text">
                        Вы можете отписаться от рассылки в любое время,
                        нажав кнопку на своем пупке.
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<footer class="footer">
    <div class="footer__container">
        <div class="footer__contacts">
            <div class="footer__phone">
                <a href="tel:{{ getSiteSettingByKey('phone') }}" class="footer__phone-num">
                    {{ getSiteSettingByKey('phone') }}
                </a>
            </div>

            <div class="footer__address">
                <svg class="icon icon-pin" width="27" height="32">
                    <use xlink:href="#icon-pin" />
                </svg>
               {{ getSiteSettingByKey('address') }}
            </div>

            <a href="mailto:{{ getSiteSettingByKey('email') }}" class="footer__mail">
                <svg class="icon icon-mail" width="26" height="26">
                    <use xlink:href="#icon-mail" />
                </svg>
                {{ getSiteSettingByKey('email') }}
            </a>
        </div>

        <div class="footer__info">
            <div class="footer__copyright">
                &copy; Концертное Агентство <b>ARTISTIKA</b>
            </div>

            <div class="footer__developer">
                СДЕЛАНО <b>OXYO</b>
            </div>
        </div>
    </div>
</footer>

